#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.22/node_modules/vue-demi/bin/node_modules:/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.22/node_modules/vue-demi/node_modules:/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.22/node_modules:/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.22/node_modules/vue-demi/bin/node_modules:/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.22/node_modules/vue-demi/node_modules:/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.22/node_modules:/mnt/c/Users/XiaoMi/Desktop/weiyi-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../bin/vue-demi-fix.js" "$@"
fi
